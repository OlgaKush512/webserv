server {
    location / {
        root /sample/
        location /aa {
        root a
        }
    }
	listen localhost
	port 8080 8081 12000
	server_name test
	error_page 404 Makefile
	root usr/site/www
	client_max_body_size 2048
	file_upload on
	autoindex on
	methods POST GET DELETE
	index index.html index.php
}
